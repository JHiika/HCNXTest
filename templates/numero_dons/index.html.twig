{% extends 'base.html.twig' %}

{% block title %}NumeroDons index{% endblock %}

{% block body %}
    <h1>NumeroDons index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numero</th>
                <th>Montant</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for numero_don in numero_dons %}
            <tr>
                <td>{{ numero_don.id }}</td>
                <td>{{ numero_don.numero }}</td>
                <td>{{ numero_don.montant }}</td>
                <td>
                    <a href="{{ path('app_numero_dons_show', {'id': numero_don.id}) }}">show</a>
                    <a href="{{ path('app_numero_dons_edit', {'id': numero_don.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_numero_dons_new') }}">Create new</a>
{% endblock %}
